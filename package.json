{
  "name": "owner-game",
  "version": "3.0.0",
  "description": "Owner game",
  "browserslist": "> 0.25%, not dead",
  "targets": {
    "edge": "17",
    "firefox": "60",
    "chrome": "67",
    "safari": "11.1",
    "ie": "11"
  },
  "scripts": {
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server --hot",
    "build": "npx del-cli dist --force  && npm run webpack -- --env.mode production",
    "dev": "npm run webpack-dev-server -- --env.mode development --watch",
    "backend": "npm install --prefix ./server && node ./server/index.js",
    "test": "npx jest",
    "code-coverage": "jest --coverage",
    "prettier": "npx prettier",
    "prettier:format": "npm run prettier -- --write src/**/*.ts",
    "prettier:check": "npm run prettier -- -c ./src/**/*.ts",
    "lint-check": "npx eslint ./src/**/*.ts",
    "code-style-check": "npm run lint-check && npm run prettier:check"
  },
  "author": "jkanchelov",
  "license": "MIT",
  "dependencies": {
    "@babel/preset-react": "^7.16.0",
    "@pixi/layers": "^1.0.6",
    "@redblobgames/dual-mesh": "git+https://github.com/pacificescape/dual-mesh",
    "@redblobgames/mapgen2": "git+https://github.com/redblobgames/mapgen2.git",
    "@redblobgames/prng": "git+https://github.com/redblobgames/prng.git",
    "core-js": "^3.6.5",
    "delaunator": "^4.0.1",
    "pixi-viewport": "^4.21.1",
    "pixi.js": "^6.0.1",
    "pixi.js-legacy": "^6.0.1",
    "points-in-polygon": "git+https://github.com/rastapasta/points-in-polygon.git",
    "poisson-disk-sampling": "^2.2.2",
    "simplex-noise": "^2.4.0",
    "stats.js": "^0.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/jest": "^26.0.14",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^14.11.8",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/stats.js": "^0.17.0",
    "@types/terser-webpack-plugin": "^4.2.0",
    "@types/webpack": "^4.41.22",
    "@types/webpack-dev-server": "^3.11.0",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.1.0",
    "babel-register": "^6.26.0",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^4.3.0",
    "del-cli": "^3.0.1",
    "es6-promise-polyfill": "^1.2.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-unicorn": "^33.0.1",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.5.3",
    "jest-canvas-mock": "^2.3.0",
    "jsdom": "^16.4.0",
    "mini-css-extract-plugin": "^1.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "terser-webpack-plugin": "^4.2.3",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "jest-canvas-mock"
    ]
  }
}
